{
    "name": "pagseguro-that-works",
    "version": "0.6.5",
    "description": "Pagseguro API wrapper that actually works!",
    "author": "Eduardo Santos Brito",
    "main": "./dist/index.js",
    "homepage": "https://github.com/EduSantosBrito/pagseguro-that-works#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EduSantosBrito/pagseguro-that-works.git"
    },
    "bugs": {
        "url": "https://github.com/EduSantosBrito/pagseguro-that-works/issues"
    },
    "types": "./dist/index.d.ts",
    "license": "MIT",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "lint-staged && yarn test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "./**/*.{ts}": [
            "yarn lint"
        ]
    },
    "scripts": {
        "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
        "dev": "nodemon",
        "lint": "eslint --ext ts ./",
        "test": "jest --verbose --runInBand",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "node-fetch": "2.6.1",
        "object-to-xml": "2.0.0",
        "rundef": "1.2.6",
        "xml2js": "0.4.23"
    },
    "devDependencies": {
        "@commitlint/cli": "12.0.1",
        "@commitlint/config-conventional": "12.0.1",
        "@types/eslint": "7.2.10",
        "@types/eslint-plugin-prettier": "3.1.0",
        "@types/expect-puppeteer": "4.4.5",
        "@types/jest": "26.0.22",
        "@types/jest-environment-puppeteer": "4.4.1",
        "@types/node": "14.14.37",
        "@types/node-fetch": "2.5.8",
        "@types/nodemon": "1.19.0",
        "@types/prettier": "2.2.1",
        "@types/puppeteer": "5.4.3",
        "@types/xml2js": "0.4.8",
        "@typescript-eslint/eslint-plugin": "4.20.0",
        "@typescript-eslint/parser": "4.20.0",
        "dotenv": "8.2.0",
        "eslint": "7.23.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-airbnb-typescript": "12.3.1",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jest": "24.3.2",
        "eslint-plugin-prettier": "3.3.1",
        "husky": "4.3.6",
        "jest": "26.6.3",
        "jest-puppeteer": "4.4.0",
        "lint-staged": "10.5.4",
        "nodemon": "2.0.7",
        "prettier": "2.2.1",
        "puppeteer": "8.0.0",
        "ts-jest": "26.5.4",
        "tsc-alias": "1.2.9",
        "typescript": "4.2.4"
    },
    "bundleDependencies": [
        "node-fetch",
        "object-to-xml",
        "rundef",
        "xml2js"
    ]
}
